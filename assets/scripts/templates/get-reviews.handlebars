<thead>
  <tr>
    <th>ID</th>
    <th>Reviews</th>
    <th>Book Title</th>
    <!-- <th></th> -->
  </tr>
</thead>
<tbody>
  {{#each reviews as |review|}}
  <tr data-id="{{review.id}}">
    <th scope="row">{{review.id}}</th>
    <td>{{review.review_content}}</td>
    <td> {{#each books as |book|}}
      {{book.title}}
      {{/each}}</td>
  </tr>
  {{/each}}
</tbody>
